{
    "name": "ChatRoom",
    "version": "1.0",
    "manifest_version": 2,

    "icons": {
        // "16": "icon_16.png",
        // "48": "icon_48.png",
        // "128": "icon_128.png"
    },

    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "ChatRoom"
    },

    "permissions": [
        "storage"
    ],

    "background": {
        "scripts": [
            "scripts/socket.io.js",
            "scripts/background.js"
        ]
    }
}